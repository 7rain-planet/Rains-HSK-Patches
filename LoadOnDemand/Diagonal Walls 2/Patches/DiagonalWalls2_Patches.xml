                    <xpath>Defs/ThingDef[defName="diagonalConduit"]</xpath>
                    <value>
                        <costList Inherit="False">
                            <Wire>1</Wire>
                        </costList>
                    </value>
                </li>

				
			</operations>
		</match>
	</Operation>

</Patch>
