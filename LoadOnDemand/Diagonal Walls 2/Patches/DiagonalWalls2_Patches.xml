<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
        <mods>
            <li>Diagonal Walls 2</li>
        </mods>
		<match Class="PatchOperationSequence">
            <operations>
                <!-- 斜铺电缆补丁 -->
